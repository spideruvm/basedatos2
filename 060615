/*
CREATE OR REPLACE PROCEDURE GUARDAR_USUARIO1(
MY_ID_USUARIO1 OUT INTEGER,
MY_NOMBRE IN VARCHAR2,
MY_EMAIL IN VARCHAR2,
MY_SUELDO_BASE IN FLOAT)AS
begin
select SEC_USUARIO1.NEXTVAL INTO MY_ID_USUARIO1 FROM dual;
INSERT INTO USUARIO1 values(
MY_ID_USUARIO1,
MY_NOMBRE,
MY_EMAIL,
MY_SUELDO_BASE);
END;
*/


CREATE OR REPLACE PROCEDURE GUARDAR_DIRECCION_USER(
MY_NOMBRE IN VARCHAR2,
MY_EMAIL IN VARCHAR2,
MY_SUELDO_BASE IN FLOAT
MY_ID_DIRECCION OUT INTEGER,
MY_ID_USUARIO INTEGER,
MY_CALLE IN VARCHAR2,
MY_NUMERO INTEGER,
MY_COLONIA varchar2,
MY_MUNICIPIO varchar2,
CP integer
)AS
ALGO INTEGER;
begin

GUARDAR_USUARIO(
ALGO,
MY_NOMBRE,
MY_EMAIL,
MY_SUELDO_BASE);

select SEC_DIRECCION.NEXTVAL INTO MY_ID_DIRECCION FROM dual;
INSERT INTO DIRECCION values(
MY_ID_DIRECCION,
ALGO,
MY_CALL,
MY_NUMERO,
MY_COLONIA,
MY_MUNICIPIO,
CP);
END;

DECLARE
VALOR INTEGER;
GUARDAR_DIRECCION_USER(('ALGO')
